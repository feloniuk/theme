{% block page_hero_section %}
<div class="hero-container" hero-game-section>
  {# Rating #}
  <div class="rating-container">
    <div class="rating-label">{{ 'mm-theme.hero.excellent'|trans|sw_sanitize }}</div>
    <div class="stars">
      {% for i in 1..5 %}
        <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
          <use xlink:href="#hero-stars-logo"></use>
        </svg>
      {% endfor %}
    </div>
    <div class="rating-text">
      <span class="rating-number">{{ 'mm-theme.hero.positiveCountPercent'|trans|sw_sanitize }} </span> {{ 'mm-theme.hero.positive'|trans|sw_sanitize }} 
      <span class="rating-number">{{ 'mm-theme.hero.reviewsCount'|trans|sw_sanitize }} </span> {{ 'mm-theme.hero.reviews'|trans|sw_sanitize }} 
    </div>
    <div class="trustpilot-logo">
      <svg width="73" height="20" viewBox="0 0 73 20" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect width="72.9911" height="16.0714" fill="url(#pattern0_4670_29737)"/>
        <defs>
        <pattern id="pattern0_4670_29737" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_4670_29737" transform="matrix(0.00550459 0 0 0.025 0.00550459 0)"/>
        </pattern>
        <image id="image0_4670_29737" width="180" height="40" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
        </defs>
      </svg>

    </div>
  </div>

  {# Hero Heading #}
  <h1 class="hero-heading">
    {{ 'mm-theme.hero.any'|trans|sw_sanitize }} <br />
    <span class="highlight">{{ 'mm-theme.hero.mmonster'|trans|sw_sanitize }}</span> {{ 'mm-theme.hero.makes'|trans|sw_sanitize }}
  </h1>

  {# Search Input #}
  <div class="search-container">
    <div class="search-input-container">
      <div class="search-inner">
        <div class="search-field">
          <svg width="21" height="20" viewBox="0 0 21 20" fill="none" xmlns="http://www.w3.org/2000/svg" style="margin-right: 6px;">
            <use xlink:href="#hero-search-logo"></use>
          </svg>
          <input type="text" class="search-main-input" placeholder="{{ 'mm-theme.hero.placeholder'|trans|sw_sanitize }}">
        </div>
        <button class="search-button">
          <svg viewBox="0 0 24 24" style="transform: rotate(3.142rad) translateY(-2px);" xmlns="http://www.w3.org/2000/svg">
              <use xlink:href="#hero-search-btn-logo"></use>
           </svg>
        </button>
      </div>
    </div>
  </div>

  {# Suggestions Component #}
  {% set suggestionsConfig = theme_config('mm-intercom-header-buttons') %}

  {% if suggestionsConfig is defined and suggestionsConfig is not empty %}
    <div class="suggestions">
      {% set customSuggestions = suggestionsConfig|split(';') %}
      
      {% if customSuggestions|length > 0 %}
        {% for suggestion in customSuggestions %}
          {% if suggestion|trim is not empty %}
            <div class="suggestion" data-suggestion-id="{{ loop.index }}">{{ suggestion|trim|trans }}</div>
          {% endif %}
        {% endfor %}
      {% endif %}
    </div>
  {% else %}
    {# Fallback suggestions if no configuration exists #}
    <div class="suggestions">
      {% set fallbackSuggestions = [
        'Get me full Mythic gear in WoW',
        'Boost my character to level 100 in Diablo 4',
        'Max out my Brawler in Brawl Stars'
      ] %}
      
      {% for suggestion in fallbackSuggestions %}
        <div class="suggestion suggestion--fallback" data-suggestion-id="{{ loop.index }}">{{ suggestion|trans }}</div>
      {% endfor %}
    </div>
  {% endif %}

